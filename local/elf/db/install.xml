<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="local/elf/db" VERSION="20120515" COMMENT="XMLDB file for Moodle local_elf"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="is_semesters" COMMENT="Semester codes from IS MU">
      <FIELDS>
      	<FIELD NAME="id" TYPE="int" LENGTH="11" NOTNULL="false" SEQUENCE="true" NEXT="code"/>
        <FIELD NAME="code" TYPE="int" LENGTH="11" NOTNULL="false" SEQUENCE="false" PREVIOUS="id" NEXT="faculty"/>
        <FIELD NAME="faculty" TYPE="char" LENGTH="5" NOTNULL="false" SEQUENCE="false" PREVIOUS="code" NEXT="period"/>
        <FIELD NAME="period" TYPE="char" LENGTH="10" NOTNULL="false" SEQUENCE="false" PREVIOUS="faculty" NEXT="year"/>
        <FIELD NAME="year" TYPE="char" LENGTH="4" NOTNULL="false" SEQUENCE="false" PREVIOUS="period"/>
      </FIELDS>
	  <KEYS>
		<KEY NAME="primary" TYPE="primary" FIELDS="id"/>
	  </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>